server {
    listen 80;
    server_name tyeshutty.tk;

    root /var/www/html/tye;

    location / {
        try_files $uri /index.html;
    }

    # location /maps {
    #     proxy_pass /;
    # }

    location "/audio" {
        proxy_pass http://unix:/home/azureuser/Audio/Audio.sock;
        proxy_connect_timeout       15;
        proxy_send_timeout          15;
        proxy_read_timeout          15;
        send_timeout                15;
    }
}
